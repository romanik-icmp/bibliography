@article{0143-0807-9-1-010,
  author={L M Quintales and J M Alvarino and S Velasco},
  title={Computational derivation of coexistence curves for van-der-Waals-type equations},
  journal={European Journal of Physics},
  volume={9},
  number={1},
  pages={55},
  url={http://stacks.iop.org/0143-0807/9/i=1/a=010},
  year={1988},
  abstract={Two computational approaches to the liquid-vapour coexistence densities and the vapour pressure for van-der-Waals-type fluids are presented. The first is based on Maxwell's area rule and the second on the properties of the molar Gibbs function. Both approaches use a simple algorithm to calculate the roots of the thermal equation of state (a cubic equation) for temperatures below critical. The coexistence curves obtained are compared with Guggenheim's empirical data and fitted to simple analytical expressions.}
}

